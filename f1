    function cargarMarcasImpresoras() {
        const galeria = document.getElementById('galeriaMarcasPrincipal');
        galeria.innerHTML = '';
        marcas.forEach(m => {
            const div = document.createElement('div');
            div.className = 'div1 div-marca';
            div.innerHTML = `<img src="${m.img}" alt="${m.name}"><p>${m.name}</p>`;
            div.onclick = () => mostrarModelosDeMarca(m.id);
            galeria.appendChild(div);
        });
    }

    function mostrarModelosDeMarca(id) {
        document.querySelector('.impresoras').style.display = 'none';
        const titulo = document.getElementById('tituloMarca');
        const lista = document.getElementById('listaModelos');
        titulo.textContent = marcas.find(m => m.id === id).name.toUpperCase();
        lista.innerHTML = '';

        Object.keys(datosModelos[id] || {}).forEach(key => {
            const mod = datosModelos[id][key];
            const div = document.createElement('div');
            div.className = 'div2';
            div.innerHTML = `<img src="${mod.img}" alt="${mod.name}"><p>${mod.name}</p>`;
            div.onclick = () => abrirModalModelo(id, key);
            lista.appendChild(div);
        });

        document.getElementById('galeriaMarcas').style.display = 'block';
    }

    function abrirModalModelo(marca, id) {
        const mod = datosModelos[marca][id];
        document.getElementById('imagenModelo').src = mod.img;
        document.getElementById('nombreModelo').textContent = mod.name;
        document.getElementById('descripcionModelo').textContent = mod.desc;
        const ul = document.getElementById('caracteristicasModelo'); ul.innerHTML = '';
        mod.features.forEach(f => { const li = document.createElement('li'); li.textContent = f; ul.appendChild(li); });
        const specs = document.getElementById('especificacionesModelo'); specs.innerHTML = '';
        for (const [k, v] of Object.entries(mod.specs)) {
            const p = document.createElement('p');
            p.innerHTML = `<strong>${k}:</strong> ${v}`;
            specs.appendChild(p);
        }
        document.getElementById('precioModelo').textContent = mod.price;
        document.getElementById('modalModelo').style.display = 'block';
    }

    function cerrarModal() {
        document.getElementById('modalModelo').style.display = 'none';
    }

    function volverPrincipal() {
        document.getElementById('galeriaMarcas').style.display = 'none';
        document.querySelector('.impresoras').style.display = 'block';
    }

    function cargarMarcasFilamentos() {
        const galeria = document.getElementById('galeriaFilamentos');
        galeria.innerHTML = '';
        marcasFilamentos.forEach(m => {
            let logoUrl;
            switch (m) {
                case 'hellbot':
                    logoUrl = 'https://tse1.mm.bing.net/th/id/OIP.O3tH5MRAEPANbx8lpTnUcAAAAA?rs=1&pid=ImgDetMain&o=7&rm=3';
                    break;
                case 'printalot':
                    logoUrl = 'https://http2.mlstatic.com/storage/mshops-appearance-api/images/18/782559318/logo-2022022117213141600.png';
                    break;
                case 'gst3d':
                    logoUrl = 'https://th.bing.com/th/id/R.adeef972fca5b406eb5431de150398ab?rik=rpEvDzqt%2b%2f1oiQ&riu=http%3a%2f%2fwww.gst3d.com%2fimages%2flogogst3.png&ehk=UoWHNG1DmsxM6xdNWfw3Mu7xumjsUK2Oh9%2b5byRnOgM%3d&risl=&pid=ImgRaw&r=0';
                    break;
                case '3n3':
                    logoUrl = 'https://admin.bless3d.com.br/public/storage/partners/April2022/6NDh8XgzIA6rjCE0GJHZ.jpg';
                    break;
                case 'grilon':
                    logoUrl = 'https://3dcasabureu.com.ar/wp-content/uploads/2021/12/Logos-carrusel_Mesa-de-trabajo-1.png';
                    break;
                default:
                    logoUrl = 'imagenes/filamentos/default.jpg';
            }

            const div = document.createElement('div');
            div.className = 'filamento-item div-marca';
            div.innerHTML = `<img src="${logoUrl}" alt="${m}"><p>${m.toUpperCase()}</p>`;
            div.onclick = () => mostrarFilamentosDeMarca(m);
            galeria.appendChild(div);
        });
    }

    function mostrarFilamentosDeMarca(marca) {
        document.querySelector('.filamentos-section').style.display = 'none';
        const titulo = document.getElementById('tituloFilamentoMarca');
        const lista = document.getElementById('listaFilamentos');
        titulo.textContent = marca.toUpperCase();
        lista.innerHTML = '';

        Object.keys(datosModelos[marca] || {}).forEach(id => {
            const fil = datosModelos[marca][id];
            const div = document.createElement('div');
            div.className = 'filamento-item';
            div.innerHTML = `<img src="${fil.img}" alt="${fil.name}"><p>${fil.name}</p>`;
            div.onclick = () => abrirModalFilamento(marca, id);
            lista.appendChild(div);
        });

        document.getElementById('galeriaFilamentosMarca').style.display = 'block';
    }

    function abrirModalFilamento(marca, id) {
        const fil = datosModelos[marca][id];
        document.getElementById('imagenFilamento').src = fil.img;
        document.getElementById('nombreFilamento').textContent = fil.name;
        document.getElementById('descripcionFilamento').textContent = fil.desc;
        const ul = document.getElementById('caracteristicasFilamento'); ul.innerHTML = '';
        fil.features.forEach(f => { const li = document.createElement('li'); li.textContent = f; ul.appendChild(li); });
        const specs = document.getElementById('especificacionesFilamento'); specs.innerHTML = '';
        for (const [k, v] of Object.entries(fil.specs)) {
            const p = document.createElement('p');
            p.innerHTML = `<strong>${k}:</strong> ${v}`;
            specs.appendChild(p);
        }
        document.getElementById('precioFilamento').textContent = fil.price;
        document.getElementById('modalFilamento').style.display = 'block';
    }

    function cerrarModalFilamento() {
        document.getElementById('modalFilamento').style.display = 'none';
    }

    function volverFilamentosPrincipal() {
        document.getElementById('galeriaFilamentosMarca').style.display = 'none';
        document.querySelector('.filamentos-section').style.display = 'block';
    }

    function mostrarRegistro() {
        document.getElementById('formularioLogin').style.display = 'none';
        document.getElementById('formularioRegistro').style.display = 'block';
    }

    function mostrarLogin() {
        document.getElementById('formularioRegistro').style.display = 'none';
        document.getElementById('formularioLogin').style.display = 'block';
    }

document.getElementById('formularioRegistro').addEventListener('submit', function(e) {
    e.preventDefault();
    const nombre = document.getElementById('nombreRegistro').value.trim().toLowerCase();
    const apellido = document.getElementById('apellidoRegistro').value.trim().toLowerCase();
    const rol = document.getElementById('rolRegistro').value;
    if (nombre && apellido) {
        const existe = usuarios.some(u => u.nombre.toLowerCase() === nombre && u.apellido.toLowerCase() === apellido);
        if (existe) {
            alert('Usuario ya registrado. Por favor, inicie sesión.');
            mostrarLogin();
        } else {
            usuarioActual = { nombre, apellido, rol };
            usuarios.push(usuarioActual);
            localStorage.setItem('usuarios', JSON.stringify(usuarios));
            localStorage.setItem('sesionActiva', 'true');
            localStorage.setItem('usuarioActual', JSON.stringify(usuarioActual));

            const pantalla = document.getElementById('pantallaLogin');
            pantalla.classList.add('desvanecer');
            setTimeout(() => {
                pantalla.style.display = 'none';
                document.getElementById('contenidoPrincipal').style.display = 'block';
                actualizarHistorial();
                window.scrollTo(0, 0);
            }, 1000);
        }
    }
});

    function actualizarHistorial() {
        const lista = document.getElementById('listaHistorial');
        lista.innerHTML = '';
        usuarios.forEach(u => {
            const li = document.createElement('li');
            li.textContent = `${u.nombre} ${u.apellido} (${u.rol || 'invitado'})`;
            lista.appendChild(li);
        });
    }

    function alternarMenu() {
        document.getElementById('menuNavegacion').classList.toggle('abierto');
    }

    function alternarHistorial() {
        document.getElementById('menuHistorial').classList.toggle('abierto');
    }

    function cerrarSesion() {
    localStorage.removeItem('sesionActiva');
    localStorage.removeItem('usuarioActual');
    usuarioActual = null;
    location.reload();
}

    document.addEventListener('DOMContentLoaded', () => {
    const sesionActiva = localStorage.getItem('sesionActiva') === 'true';
    const usuarioGuardado = localStorage.getItem('usuarioActual');

    if (sesionActiva && usuarioGuardado) {
        usuarioActual = JSON.parse(usuarioGuardado);
        usuarios.push(usuarioActual);
        document.getElementById('pantallaLogin').style.display = 'none';
        document.getElementById('contenidoPrincipal').style.display = 'block';
        actualizarHistorial();
        window.scrollTo(0, 0);
    }

    cargarMarcasImpresoras();
    cargarMarcasFilamentos();

    if (localStorage.getItem('usuarios')) {
        actualizarHistorial();
    }
});

let carrito = [];

document.addEventListener('DOMContentLoaded', () => {
    const guardado = localStorage.getItem('carrito');
    if (guardado) {
        carrito = JSON.parse(guardado);
        actualizarCarrito();
    }
});

function agregarAlCarrito(tipo, marca, nombre, precio) {
    carrito.push({ tipo, marca, nombre, precio });
    localStorage.setItem('carrito', JSON.stringify(carrito));
    actualizarCarrito();
    alert(`${nombre} agregado al carrito`);
}

function removerDelCarrito(index) {
    carrito.splice(index, 1);
    localStorage.setItem('carrito', JSON.stringify(carrito));
    actualizarCarrito();
}

function actualizarCarrito() {
    const lista = document.getElementById('listaCarrito');
    const totalEl = document.getElementById('totalCarrito');
    const contador = document.getElementById('contadorCarrito');
    
    lista.innerHTML = '';
    let total = 0;

    carrito.forEach((item, i) => {
        const li = document.createElement('li');
        li.innerHTML = `
            <strong>${item.marca}</strong><br>
            ${item.nombre}<br>
            $<span class="precio-item">${item.precio.toLocaleString('es-AR')}</span> ARS
            <button onclick="removerDelCarrito(${i})" class="btn-eliminar">×</button>
        `;
        lista.appendChild(li);
        total += item.precio;
    });

    totalEl.textContent = total.toLocaleString('es-AR');
    contador.textContent = carrito.length;
}

function alternarCarrito() {
    document.getElementById('menuCarrito').classList.toggle('abierto');
}

function procesarCompra() {
    if (carrito.length === 0) {
        alert('El carrito está vacío');
        return;
    }
    alert(`Compra procesada por $${document.getElementById('totalCarrito').textContent} ARS`);
    carrito = [];
    localStorage.removeItem('carrito');
    actualizarCarrito();
    alternarCarrito();
}
document.getElementById('formularioLogin').addEventListener('submit', function(e) {
    e.preventDefault();
    const nombre = document.getElementById('nombreLogin').value.trim().toLowerCase();
    const apellido = document.getElementById('apellidoLogin').value.trim().toLowerCase();
    if (nombre && apellido) {
        const existe = usuarios.some(u => u.nombre.toLowerCase() === nombre && u.apellido.toLowerCase() === apellido);
        if (existe) {
            usuarioActual = usuarios.find(u => u.nombre.toLowerCase() === nombre && u.apellido.toLowerCase() === apellido);
            localStorage.setItem('sesionActiva', 'true');
            localStorage.setItem('usuarioActual', JSON.stringify(usuarioActual));

            document.getElementById('fotoUsuarioLogin').src = `imagenes/usuario/${nombre}_${apellido}.jpg`;

            const pantalla = document.getElementById('pantallaLogin');
            pantalla.classList.add('desvanecer');
            setTimeout(() => {
                pantalla.style.display = 'none';
                document.getElementById('contenidoPrincipal').style.display = 'block';
                actualizarHistorial();
                window.scrollTo(0, 0);
            }, 1000);
        } else {
            alert('Usuario no registrado. Por favor, regístrese.');
            mostrarRegistro();
        }
    }
});
</script>


</body>
</html>
